{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "outDir": "build",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
